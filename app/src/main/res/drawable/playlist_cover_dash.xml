<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <size
                android:width="360dp"
                android:height="360dp" />
            <stroke
                android:width="1dp"
                android:color="@color/yp_text_gray"
                android:dashWidth="@dimen/dash_gap"
                android:dashGap="@dimen/dash_gap" />
            <corners android:radius="@dimen/corner8" />
        </shape>
    </item>

    <item
        android:drawable="@drawable/ic_playlist_holder"
        android:gravity="center" />
</layer-list>